<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.termproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TermProject">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp" />
        <activity android:name=".SectionRUD" />
        <activity android:name=".SectionRegistrationForm" />
        <activity android:name=".SectionList" />
        <activity android:name=".CourseRUD" />
        <activity android:name=".CourseRegistrationForm" />
        <activity android:name=".StudentRUD" />
        <activity android:name=".StudentRegistrationForm" />
        <activity android:name=".StudentList" />
        <activity android:name=".SessionRUD" />
        <activity android:name=".SessionRegistrationForm" />
        <activity android:name=".SessionList" />
        <activity android:name=".CoursesList" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>